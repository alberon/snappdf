#!/usr/bin/env php
<?php

require __DIR__ . '/vendor/autoload.php';

use Beganovich\ChromiumPdf\Command\DownloadChromiumCommand;
use Beganovich\ChromiumPdf\Command\UpdateActiveBinary;
use Symfony\Component\Console\Application;

$application = new Application('ChromiumPdf');

$application->add(new DownloadChromiumCommand());
$application->add(new UpdateActiveBinary());

$application->run();
